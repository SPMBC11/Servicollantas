{
    "roots": ["project/frontend"],
  "buildCommand": "cd project/frontend && npm install && npm run build",  "outputDirectory": "project/frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://serviciollantas.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
